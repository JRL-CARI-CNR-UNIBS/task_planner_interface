<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <RetryUntilSuccessful num_attempts="-1a">
            <Inverter>
                <Action ID="SingleTaskDispatcher" agent_name="" piece_input="" piece_output="" task_name=""/>
            </Inverter>
        </RetryUntilSuccessful>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="SingleTaskDispatcher">
            <input_port name="agent_name"/>
            <input_port name="piece_input"/>
            <output_port name="piece_output"/>
            <input_port name="task_name"/>
        </Action>
        <Condition ID="TriggerNewPiece">
            <output_port name="piece_output"/>
        </Condition>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

